[user]
  email = derrick@track5media.com
  name = Derrick Nelson
[color]
  ui = auto
[color "diff"]
  meta = cyan bold
  frag = yellow bold
  old = red bold
  new = green bold
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[alias]
  co = checkout
  ci = commit
  st = status -sb
  br = branch
[core]
  pager = less -x1,5
